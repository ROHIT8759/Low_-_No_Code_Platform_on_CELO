# Direct connection to the database (used for Prisma migrations)
DIRECT_URL="postgresql://postgres.YOUR_PROJECT_REF:YOUR_PASSWORD@aws-1-ap-south-1.pooler.supabase.com:5432/postgres"

# Supabase Client (for RLS / realtime features)
NEXT_PUBLIC_SUPABASE_URL="https://YOUR_PROJECT_REF.supabase.co"
